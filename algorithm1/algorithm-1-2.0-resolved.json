{
  "openapi": "3.0.0",
  "info": {
    "title": "algorithm-1",
    "description": "This is the Algorithm-1 API endpoint specs for SENG499 Capstone Company 3 & 4.",
    "version": "2.0"
  },
  "servers": [
    {
      "url": "https://virtserver.swaggerhub.com/GAGNE16/algorithm-1/1.0"
    }
  ],
  "paths": {
    "/schedule": {
      "post": {
        "summary": "Generate schedule(s)",
        "description": "Takes 3 inputs and returns 1 to 3 semesters for Fall, Spring, and Summer terms accordingly. Would return semester schedules for filled fields. Therefore, if fall is the only field then the other will be empty.",
        "requestBody": {
          "description": "This is the request body format.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/schedule_body"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Schedule(s) generated successfully and returned.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "example": {
                    "fallCourses": [],
                    "SpringCourses": [],
                    "SummerCourses": []
                  },
                  "items": {
                    "$ref": "#/components/schemas/Schedule"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Error generating schedule(s)."
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Schedule": {
        "required": [
          "FallCourses",
          "SprintCourses",
          "SummerCourses"
        ],
        "type": "object",
        "properties": {
          "fallCourses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Course"
            }
          },
          "springCourses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Course"
            }
          },
          "summerCourses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Course"
            }
          }
        }
      },
      "Course": {
        "required": [
          "CourseNumber",
          "CourseTitle",
          "SequenceNumber",
          "Subject"
        ],
        "type": "object",
        "properties": {
          "CourseNumber": {
            "type": "string",
            "example": "111"
          },
          "Subject": {
            "type": "string",
            "example": "CSC"
          },
          "SquenceNumber": {
            "type": "string",
            "example": "A01"
          },
          "CourseTitle": {
            "type": "string",
            "example": "Fundamentals of Programming with Engineering Applications"
          },
          "Assignment": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Assignment"
            }
          },
          "Prof": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Professor"
            }
          }
        }
      },
      "Assignment": {
        "required": [
          "BeginTime",
          "EndDate",
          "EndTime",
          "Friday",
          "HoursWeek",
          "Monday",
          "Saturday",
          "StartDate",
          "Sunday",
          "Thursday",
          "Tuesday",
          "Wednesday"
        ],
        "type": "object",
        "properties": {
          "StartDate": {
            "type": "string",
            "example": "Jan 07, 2019"
          },
          "EndDate": {
            "type": "string",
            "example": "Apr 05, 2019"
          },
          "BeginTime": {
            "type": "string",
            "example": "0830"
          },
          "EndTime": {
            "type": "string",
            "example": "0930"
          },
          "HoursWeek": {
            "type": "integer",
            "example": 3
          },
          "Sunday": {
            "type": "boolean",
            "example": false
          },
          "Monday": {
            "type": "boolean",
            "example": true
          },
          "Tuesday": {
            "type": "boolean",
            "example": false
          },
          "Wednesday": {
            "type": "boolean",
            "example": false
          },
          "Thursday": {
            "type": "boolean",
            "example": true
          },
          "Friday": {
            "type": "boolean",
            "example": false
          },
          "Saturday": {
            "type": "boolean",
            "example": false
          }
        }
      },
      "Professor": {
        "required": [
          "DisplayName",
          "Preferences"
        ],
        "type": "object",
        "properties": {
          "Preferences": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Preference"
            }
          },
          "DisplayName": {
            "type": "string",
            "example": "Michael, Zastre"
          },
          "FallTermCourses": {
            "type": "integer",
            "example": 1
          },
          "SpringTermCourses": {
            "type": "integer",
            "example": 1
          },
          "SummerTermCourses": {
            "type": "integer",
            "example": 1
          }
        }
      },
      "Preference": {
        "required": [
          "CourseNum",
          "PreferenceNum"
        ],
        "type": "object",
        "properties": {
          "CourseNum": {
            "type": "string",
            "example": "CSC111"
          },
          "PreferenceNum": {
            "type": "integer",
            "example": 0
          },
          "Term": {
            "type": "string",
            "enum": [
              "FALL",
              "SPRING",
              "SUMMER"
            ]
          }
        }
      },
      "schedule_body": {
        "type": "object",
        "properties": {
          "hardScheduled": {
            "type": "array",
            "example": {
              "fallCourses": [],
              "SpringCourses": [],
              "SummerCourses": []
            },
            "items": {
              "$ref": "#/components/schemas/Schedule"
            }
          },
          "coursesToSchedule": {
            "type": "array",
            "example": {
              "fallCourses": [],
              "SpringCourses": [],
              "SummerCourses": []
            },
            "items": {
              "$ref": "#/components/schemas/Schedule"
            }
          },
          "professors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Professor"
            }
          }
        }
      }
    }
  }
}